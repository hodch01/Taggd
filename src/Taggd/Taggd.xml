<?xml version="1.0" encoding="utf-8" ?>
<widget id="Taggd.widget.Taggd" needsEntityContext="true" xmlns="http://www.mendix.com/widget/1.0/">
    <name>Taggd</name>
    <description>The description of this widget.</description>

    <icon></icon>

    <properties>
        <property key="backgroundImage" type="image" required="true">
            <caption>Background image</caption>
            <category>Behavior</category>
            <description>The image for which notes will be added.</description>
        </property>
        <property key="arrayString" type="attribute" required="true">
            <caption>Store attribute</caption>
            <category>Behavior</category>
            <description>This field to store the string representation of the array</description>
            <attributeTypes>
                <attributeType name="String" />
            </attributeTypes>
        </property>
    </properties>
</widget>

